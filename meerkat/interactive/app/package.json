{
    "name": "@meerkat-ml/meerkat",
    "version": "0.4.0",
    "description": "The Meerkat Svelte Library",
    "scripts": {
        "dev": "vite dev",
        "build": "VITE_API_URL_PLACEHOLDER=http://meerkat.dummy vite build",
        "build:verbose": "VITE_API_URL_PLACEHOLDER=http://meerkat.dummy vite build --debug",
        "livebuild": "VITE_API_URL_PLACEHOLDER=http://meerkat.dummy vite build --watch",
        "package": "svelte-package; npm run package:tailwind;",
        "package:tailwind": "npx tailwindcss -i ./src/lib/app.css -o ./package/app.css",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. ."
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/adapter-static": "^1.0.0-next.48",
        "@sveltejs/kit": "next",
        "@sveltejs/package": "^1.0.0-next.6",
        "@svelteuidev/core": "^0.7.7",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@types/d3": "^7.4.0",
        "@types/d3-quadtree": "^3.0.2",
        "@types/d3-selection": "^3.0.3",
        "@types/jest": "^29.2.6",
        "@types/marked": "^4.0.7",
        "@types/prismjs": "^1.26.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vitest/ui": "^0.28.3",
        "autoprefixer": "latest",
        "babel-jest": "^29.3.1",
        "cross-fetch": "^3.1.5",
        "esbuild": "^0.13.4",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jsdom": "^21.1.0",
        "msw": "^1.0.0",
        "postcss": "latest",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte-headless-table": "^0.17.2",
        "svelte-jester": "^2.3.2",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "latest",
        "ts-jest": "^29.0.5",
        "tslib": "^2.3.1",
        "typescript": "^4.8.4",
        "vite": "^3.0.0",
        "vitest": "^0.28.3"
    },
    "type": "module",
    "dependencies": {
        "@datastructures-js/priority-queue": "^6.1.2",
        "@magidoc/plugin-svelte-marked": "^3.2.1",
        "@svelteuidev/composables": "^0.7.7",
        "@types/underscore": "^1.11.4",
        "@zerodevx/svelte-json-view": "^1.0.1",
        "carbon-icons-svelte": "^11.2.0",
        "d3": "^7.6.1",
        "d3-brush": "^3.0.0",
        "d3-quadtree": "^3.0.1",
        "d3-scale-chromatic": "^3.0.0",
        "d3-selection": "^3.0.0",
        "flowbite": "^1.5.2",
        "flowbite-svelte": "^0.29.11",
        "layercake": "^7.0.0",
        "marked": "^4.2.12",
        "monaco-editor": "^0.34.1",
        "pdfjs-dist": "^3.3.122",
        "prismjs": "^1.28.0",
        "sanitize-html": "^2.8.1",
        "svelte": "^3.54.0",
        "svelte-bootstrap-icons": "^2.2.0",
        "svelte-check": "^2.7.1",
        "svelte-dnd-action": "^0.9.19",
        "svelte-forms": "^2.3.1",
        "svelte-french-toast": "^1.0.3",
        "svelte-grid": "^5.1.1",
        "svelte-heros": "^2.3.3",
        "svelte-highlight": "^6.2.0",
        "svelte-jsx": "^2.0.0",
        "svelte-loading-spinners": "^0.1.7",
        "svelte-modals": "^1.1.0",
        "svelte-popperjs": "^1.2.11",
        "svelte-select": "^5.0.0",
        "svelte-tippy": "^1.3.2",
        "svelte-tooltip": "^1.2.0",
        "svelte-vega": "^1.2.0",
        "svelte-zoom": "^1.0.16",
        "tippy.js": "^6.3.7",
        "underscore": "^1.13.4",
        "uuid": "^9.0.0",
        "uuid-int": "^3.1.0"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.ts?$": "ts-jest",
            "^.+\\.svelte$": "svelte-jester"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
        "moduleFileExtensions": [
            "ts",
            "js",
            "svelte"
        ],
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "@testing-library/jest-dom/extend-expect"
        ]
    }
}
